        <field name="street">8 Hanover Quay</field>
        <field name="city">Dublin</field>
        <field name="phone">(+34) 91 123 45 67.</field>
        <field name="country_id" ref="base.ir" />
        <field name="email">info@airbnb.com</field>
        <field name="website">http://www.airbnb.com</field>
        <field
            name="image_1920"
            type="base64"
            file="roomdoo_saas_preconfig/static/img/airbnb-logo.svg"
        />
        <field name="property_product_pricelist" ref="tarifa_estandar" />
    </record>
