<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <!-- OTAs -->
    <record id="agency_booking" model="res.partner">
        <field name="is_agency">True</field>
        <field
            name="sale_channel_id"
            ref="connector_pms_wubook.main_pms_sale_channel_wubook_ota"
        />
        <field name="name">Booking.com B.V.</field>
        <field name="street">Herengracht 597</field>
        <field name="city">Amsterdam</field>
        <field name="phone">+31 20 712 5609</field>
        <field name="country_id" ref="base.ne" />
        <field name="email">info@booking.com</field>
        <field name="website">http://www.booking.com</field>
        <field
            name="image_1920"
            type="base64"
            file="roomdoo_saas_preconfig/static/img/booking-logo.svg"
        />
        <field name="property_product_pricelist" ref="tarifa_estandar" />
    </record>
