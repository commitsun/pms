        <field
            name="pms_amenity_type_id"
            ref="roomdoo_saas_preconfig.roomdoo_amenity_type_bed_type"
        />
        <field name="is_add_code_room_name">True</field>
        <field name="default_code">TW</field>
    </record>
    <record id="roomdoo_amenity_m_bed" model="pms.amenity">
        <field name="name">Matrimonio</field>
        <field
            name="pms_amenity_type_id"
            ref="roomdoo_saas_preconfig.roomdoo_amenity_type_bed_type"
        />
        <field name="is_add_code_room_name">True</field>
        <field name="default_code">M</field>
    </record>
</odoo>
